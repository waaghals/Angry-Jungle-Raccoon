<h1>@ViewBag.Title</h1>
<p>
    @Html.Raw(ViewBag.Description)
</p>
<h2>Mededelingen</h2>
<p>
    @{
        foreach(String s in ViewBag.Mededelingen)
        {
            @Html.Raw(s);
        <br />
        }
        
    }
</p>
    <table>
        <tr>
            <td>
                <select name="beoordelingsmoment" class="fancy_selectbox" tabindex="1">
                    @{
                        int i = 1;
                        foreach(String s in ViewBag.BeoordelingsMomenten)
                        {
			                <option value="@i">@s</option>
            
                            i++;
                        }
                    }
                </select> 
            </td>
            <td><button class="button" style ="">Medestudent beoordelen</button></td>
        </tr>
        <tr>
            <td><button class="button" style ="">Beoordelingen bekijken</button></td>
        </tr>
    </table>
    <br />
    
